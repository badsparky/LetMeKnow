<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="LetMeKnow.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:LetMeKnow"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="Controler"
            Content="{Binding MainControler}"/>
        <ShellContent
            Title="Setter"
            Content="{Binding GoalSetter}" />
    </TabBar>

</Shell>
